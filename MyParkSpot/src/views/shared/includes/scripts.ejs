<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

<% if(messages.success) { %>
<script>
  Swal.fire({
    title: 'Success!',
    text: '<%= messages.success %>',
    icon: 'success',
  });
</script>
<% } else if(messages.error) { %>
<script>
  Swal.fire({
    title: 'Error!',
    text: '<%= messages.error %>',
    icon: 'error',
  });
</script>
<% } %>
